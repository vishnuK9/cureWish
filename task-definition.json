{
    "family": "ecs-demo-task",
    "containerDefinitions": [
        {
            "image": "vishnu99docker/apache_image:",
            "name": "ecs-apache",
	    "essential": true,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ]
        }
    ],
    "cpu": "256",
    "memory": "512",
    "requiresCompatibilities": [
             "FARGATE"
    ],
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::518234350460:role/ecsTaskExecutionRole" 
}
